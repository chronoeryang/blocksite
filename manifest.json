{
  "manifest_version": 2,
  "name": "BlockSite",
  "description": "Block site",
  "version": "0.0.4",
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "background",
    "tabs",
    "activeTab",
    "*://*.securedwebsearch.com/*",
    "*://*.search.yahoo.com/*"
  ],
  "background": {
    "scripts": ["execute.js"],
    "persistent": true
  }
}
